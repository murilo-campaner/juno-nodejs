{
  "name": "juno-nodejs",
  "version": "1.0.12",
  "description": "Esse projeto destina-se a pessoas que estão realizando a integração com a API de pagamentos [JUNO](https://juno.com.br/) e estão encontrando problemas para realizar o hash do cartão de crédito no Frontend.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "postinstall": "patch-package",
    "start": "tsc -w",
    "prepare": "npm run build",
    "prepublishOnly" : "npm run test && npm run lint",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "author": "Murilo Campaner",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "base-64": "^0.1.0",
    "isomorphic-webcrypto": "^2.3.6",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/base-64": "^0.1.3",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.12",
    "@types/qs": "^6.9.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murilo-campaner/juno-nodejs.git"
  },
  "keywords": [
    "juno",
    "integracao",
    "api",
    "nodejs",
    "react-native",
    "cardhash",
    "card",
    "hash",
    "pagamentos"
  ],
  "bugs": {
    "url": "https://github.com/murilo-campaner/juno-nodejs/issues"
  },
  "homepage": "https://github.com/murilo-campaner/juno-nodejs#readme"
}
